{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Whiteboard":{
    "RepoFolder": "BoardStorage",
    "AutoCommitInterval": 5000
  },
  "Serilog": {
    "WriteTo": [
      { "Name": "File", "Args": { "path": "whiteboard.log", "rollingInterval": "Day" } }      
    ]    
  }
}
